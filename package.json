{
  "name": "clawdex",
  "version": "0.1.0",
  "description": "Solana DEX trading CLI powered by Jupiter",
  "type": "module",
  "bin": {
    "clawdex": "dist/cli.js"
  },
  "files": [
    "dist/"
  ],
  "scripts": {
    "start": "node dist/cli.js",
    "dev": "bun run src/cli.ts",
    "build": "tsc",
    "prepublishOnly": "tsc",
    "test": "bun test",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "solana",
    "dex",
    "jupiter",
    "swap",
    "cli",
    "trading"
  ],
  "author": "JoelCCodes",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/JoelCCodes/ClawDex.git"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@solana/web3.js": "^1.98",
    "@solana/spl-token": "^0.4",
    "commander": "^13",
    "@iarna/toml": "^2.2",
    "chalk": "^5"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^25.2.3",
    "@typescript-eslint/eslint-plugin": "^8",
    "@typescript-eslint/parser": "^8",
    "eslint": "^9",
    "typescript": "^5.7"
  }
}
